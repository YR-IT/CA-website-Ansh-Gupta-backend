{
  "name": "ca-project-backend",
  "version": "1.0.0",
  "description": "Backend API for A S GUPTA AND CO - Chartered Accountants Website",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup:admin": "node scripts/setupAdmin.js",
    "seed:services": "node -e \"require('dotenv').config(); require('./config/db')().then(() => require('./utils/seedServices').seedServices().then(() => process.exit(0)))\"",
    "seed:services:images": "node -e \"require('dotenv').config(); require('./config/db')().then(() => require('./utils/seedServices').seedServices(true).then(() => process.exit(0)))\"",
    "generate:secret": "node -e \"console.log('JWT_SECRET=' + require('crypto').randomBytes(64).toString('hex'))\""
  },
  "keywords": [
    "chartered-accountant",
    "tax-services",
    "accounting",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "A S GUPTA AND CO",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "slugify": "^1.6.6",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}
